# Utils
alias mkdir='mkdir -p'
alias no='nodemon -e js,coffee,json --verbose'
alias watch='watch --no-title'

# apps
alias g="git"
alias be="bundle exec"
alias t="tmux"
alias ta="tmux attach"
alias tl="tmux list-sessions"
alias tk="tmux kill-session -t"
alias kt="killall tmux"
alias wr="wr --exec"
alias ip="curl ifconfig.me/ip"
alias tnd="tmux_name_window_after_directory"

# Versal
alias v="time versal"
alias vp="time versal preview"
alias w="web"
